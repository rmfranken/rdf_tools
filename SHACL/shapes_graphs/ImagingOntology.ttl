    sh:class schema:Organization ;
    sh:description "Link to the organization funding a software component"@en ;
    sh:name "funding source"@en .

sd:FundingInformation
    a sh:NodeShape, rdfs:Class ;
    sh:description "A class to represent the funding information of a software project"@en ;
    sh:name "Funding Information"@en ; 
    sh:property schema:Thing-identifier,
                sd:FundingInformation-fundingGrant,
                sd:FundingInformation-fundingSource ;
    .

sd:DatasetSpecification
    a sh:NodeShape, rdfs:Class ;
    rdfs:subClassOf <http://purl.org/linked-data/cube#DataStructureDefinition> ;
    sh:description "Class designed to describe a type of input or output used or produced by a model. For example, Topoflow has several inputs. One of them is a text file with precipitation values. The representation of this input is an instance of a dataset specification."@en ;
    sh:name "Dataset Specification"@en ;
    sh:property schema:Thing-description ,
                schema:CreativeWork-encodingFormat,
                schema:Thing-name,
                sd:DatasetSpecification-hasDimensionality,
                sd:DatasetSpecification-hasFormat ;
                sh:property schema:PropertyValueSpecification-defaultValue, 
                schema:PropertyValueSpecification-valueRequired
    .

ren:CommandInput
    a sh:NodeShape, rdfs:Class ;
    sh:description "Input of a command line run"@en ;
