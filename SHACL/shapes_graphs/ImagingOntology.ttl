  sh:datatype xsd:string ;
.

schema:Thing-image
  a sh:PropertyShape ;
  sh:path schema:image ;
  sh:description "An image of the item. This can be a [[URL]] or a fully described [[ImageObject]]."^^rdf:HTML ;
  sh:name "image" ;
  sh:or (
      [
        sh:class schema:ImageObject ;
      ]
      [
        sh:nodeKind sh:IRI ;
      ]
    ) ;
.

