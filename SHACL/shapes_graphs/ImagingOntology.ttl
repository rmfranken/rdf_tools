schema:SoftwareApplication-applicationCategory
  a sh:PropertyShape ;
  sh:path schema:applicationCategory ;
  sh:description "Type of software application, e.g. 'Game, Multimedia'."^^rdf:HTML ;
  sh:name "applicationCategory" ;
  sh:or (
      [
        sh:nodeKind sh:IRI ;
      ]
      [
        sh:datatype xsd:string ;
      ]
    ) ;
.

